#+BEGIN_SRC python :results graphics file :file bot1.png :session *Python*
  import matplotlib.pyplot as plt

  # create some data
  x = [1, 2, 3, 4, 5]
  y = [2, 4, 6, 8, 10]

  # create a line plot
  plt.plot(x, y)

  # display the plot
  plt.savefig("bot1.png")
